    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "jest": "^29.3.1",
    "mssql": "^9.0.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
