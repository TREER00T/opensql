{
  "name": "opensql",
  "version": "1.2.47",
  "description": "OpenSql is a promise-based Node.js ORM tool for MySql",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "tsc index.ts --outDir .dist",
    "start": "node .dist/index.js",
    "build": "tsc --module CommonJS --outDir dist",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treegex/opensql.git"
  },
  "keywords": [
    "orm",
    "opensql",
    "mysql-orm",
    "mysql",
    "javascript",
    "node.js"
  ],
  "devDependencies": {
    "jest": "^28.1.1",
    "mssql": "^9.0.1",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0"
  },
  "author": "Ali Azmoodeh <treeroot.ir@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/treegex/opensql/issues"
  },
  "homepage": "https://github.com/treegex/opensql#readme"
